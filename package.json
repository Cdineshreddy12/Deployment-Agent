{
  "name": "deployment-agent",
  "version": "1.0.0",
  "description": "AI-Powered Deployment Automation Platform",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "dev:backend": "cd backend && PORT=5002 ALLOWED_ORIGINS=http://localhost:5001,http://localhost:5002 nodemon server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "install:all": "npm install && cd frontend && npm install",
    "mcp": "node backend/mcp/index.js",
    "mcp:dev": "nodemon backend/mcp/index.js",
    "test:mcp": "node backend/tests/mcp.test.js",
    "migrate:fix-deployment-env-indexes": "node scripts/fix-deployment-env-indexes.js"
  },
  "keywords": [
    "deployment",
    "terraform",
    "aws",
    "claude",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@aws-sdk/client-ec2": "^3.700.0",
    "@aws-sdk/client-ecr": "^3.700.0",
    "@aws-sdk/client-ecs": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-cloudwatch": "^3.700.0",
    "@aws-sdk/client-cost-explorer": "^3.700.0",
    "@aws-sdk/client-eks": "^3.700.0",
    "@aws-sdk/client-sts": "^3.700.0",
    "@kubernetes/client-node": "^0.21.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@slack/web-api": "^6.11.0",
    "aws-sdk": "^2.1519.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.9",
    "dotenv": "^16.3.1",
    "eventsource": "^2.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fs-extra": "^11.3.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.12",
    "simple-git": "^3.30.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
